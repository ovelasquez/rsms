<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Envios" table="envios">
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="rut" type="string" column="rut" length="60" nullable="false"/>
    <field name="nombre" type="string" column="nombre" length="60" nullable="false"/>
    <field name="apellido" type="string" column="apellido" length="60" nullable="false"/>
    <field name="fono" type="string" column="fono" length="20" nullable="false"/>
    <field name="oferta" type="string" column="oferta" length="160" nullable="false"/>
    <field name="fechaOferta" type="datetime" column="fecha_oferta" nullable="false"/>
    <field name="avisoid" type="bigint" column="avisoid" nullable="false"/>
    <field name="personaid" type="bigint" column="personaid" nullable="false"/>
    <field name="dominioid" type="bigint" column="dominioid" nullable="false"/>
    <field name="empresaid" type="bigint" column="empresaid" nullable="false"/>
    <field name="empresa" type="string" column="empresa" length="50" nullable="true"/>
    <field name="status" type="smallint" column="status" nullable="false"/>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="statusEnrolamiento" type="smallint" column="status_enrolamiento" nullable="false"/>
    <field name="codigo" type="string" column="codigo" length="10" nullable="false"/>
    <field name="idOpratel" type="string" column="id_opratel" length="16" nullable="true"/>
    <field name="repuestaOpratel" type="string" column="repuesta_opratel" length="255" nullable="true"/>
    <field name="porEnvio" type="smallint" column="por_envio" nullable="false"/>
  </entity>
</doctrine-mapping>
